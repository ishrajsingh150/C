#include <stdio.h>
int main() {
    int a = 10, b = 3;

    // Arithmetic
    printf("Arithmetic: %d %d %d %d %d\n", a+b, a-b, a*b, a/b, a%b);

    // Relational
    printf("Relational: %d %d %d %d %d %d\n", a==b, a!=b, a>b, a<b, a>=b, a<=b);

    // Logical
    printf("Logical: %d %d %d\n", (a && b), (a || b), (!a));

    // Conditional
    int max = (a>b) ? a : b;
    printf("Conditional (max): %d\n", max);
    return 0;
}
