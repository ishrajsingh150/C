#include <stdio.h>

int main() {
    int ch;
    printf("1=SI  2=Prime  3=Armstrong\n");
    scanf("%d",&ch);

    if(ch==1){
        float p,r,t;
        scanf("%f%f%f",&p,&r,&t);
        printf("%.2f",(p*r*t)/100);
    }

    else if(ch==2){
        int n,i,flag=1;
        scanf("%d",&n);
        for(i=2;i*i<=n;i++) if(n%i==0) flag=0;
        printf(flag? "Prime":"Not Prime");
    }

    else if(ch==3){
        int n,orig,sum=0,d;
        scanf("%d",&n);
        orig=n;
        while(n){ d=n%10; sum+=d*d*d; n/=10;}
        printf(sum==orig? "Armstrong":"Not Armstrong");
    }

    return 0;
}
