#include <stdio.h>

int mystrcmp(char *x,char *y){
    while(*x && *y && *x==*y) x++,y++;
    return *x-*y;
}

void mystrcpy(char *d,char *s){
    while((*d++ = *s++));
}

void mystrcat(char *d,char *s){
    while(*d) d++;
    while((*d++ = *s++));
}

int main() {
    char a[50]="Hello", b[50]="World";

    mystrcat(a,b);
    printf("%s\n",a);

    mystrcpy(a,"Test");
    printf("%s\n",a);

    printf("%d\n", mystrcmp("abc","abd"));

    return 0;
}
